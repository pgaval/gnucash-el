msgstr ""

#: Help_ch_Customize.xml:750(guilabel)
msgid "Transaction Editor Defaults"
msgstr ""

#: Help_ch_Customize.xml:754(para)
msgid "Auto-create new transactions: Set the 'auto-create' flag on newly created scheduled transactions."
msgstr ""

#: Help_ch_Customize.xml:759(para)
msgid "Notify before transactions are created: Set the 'notify' flag on newly created scheduled transactions."
msgstr ""

#: Help_ch_Customize.xml:764(para)
