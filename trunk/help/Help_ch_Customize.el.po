msgid "This is an screen image of the \"Date/Time Preferences\" windows."
msgstr ""

#: Help_ch_Customize.xml:382(para)
msgid "<guilabel>Time Format:</guilabel> Check-box to use a 24 hour (instead of a 12 hour) time format."
msgstr ""

#: Help_ch_Customize.xml:387(para)
msgid "<guilabel>Date</guilabel> - Radio Button list to select one of the following;"
msgstr ""

#: Help_ch_Customize.xml:392(para)
msgid "Locale: Use the date format specified by the system locale."
msgstr ""

#: Help_ch_Customize.xml:396(para)
msgid "US: Use the date format common in the United States."
msgstr ""

#: Help_ch_Customize.xml:400(para)
msgid "UK: Use the date format common in the United Kingdom."
msgstr ""

#: Help_ch_Customize.xml:404(para)
msgid "Europe: Use the date format common in continental Europe."
msgstr ""

#: Help_ch_Customize.xml:408(para)
msgid "ISO: Use the date format specified by the ISO-8601 standard."
msgstr ""

#: Help_ch_Customize.xml:418(title)
#: Help_ch_Customize.xml:424(guilabel)
#: Help_ch_Customize.xml:986(title)
#: Help_ch_Customize.xml:1116(title)
msgid "General"
msgstr ""

#: Help_ch_Customize.xml:428(para)
msgid "Display \"Tip of the Day\" dialog: Display hints for using GnuCash at start-up."
msgstr ""

#: Help_ch_Customize.xml:433(para)
msgid "Preform Account list setup on new file: Present the new account list dialog when you choose \"New File\" from the \"File\" menu"
msgstr ""

#: Help_ch_Customize.xml:438(para)
msgid "Show close button on Notebook tab: Show a close button on each notebook tab. These function identically to the 'Close' menu item."
msgstr ""

#: Help_ch_Customize.xml:443(para)
msgid "Enable euro support: Enables support for the European Union EURO currency."
msgstr ""

#: Help_ch_Customize.xml:450(guilabel)
msgid "Numbers:"
msgstr ""

#: Help_ch_Customize.xml:454(para)
msgid "Display negative amounts in red:"
msgstr ""

#: Help_ch_Customize.xml:458(para)
msgid "Automatic Decimal Point: Automatically insert a decimal point into values that are entered without one."
msgstr ""

#: Help_ch_Customize.xml:463(para)
msgid "Auto Decimal Places: Select the number of decimal places that will be inserted automatically."
msgstr ""

#: Help_ch_Customize.xml:470(guilabel)
msgid "Files:"
msgstr ""

#: Help_ch_Customize.xml:474(para)
msgid "Compress files: Checkbox to compress the data file with gzip when saving it to disk."
msgstr ""

#: Help_ch_Customize.xml:479(para)
msgid "Retain log files: Select the number of days to save log/backup files. Delete old log/backup files after this many days (0 = never)."
msgstr ""

#: Help_ch_Customize.xml:486(guilabel)
msgid "Search Dialog:"
msgstr ""

#: Help_ch_Customize.xml:490(para)
msgid "New search limit: Default to 'new search' if fewer than this number of items is returned."
msgstr ""

#: Help_ch_Customize.xml:500(title)
msgid "On-line Banking"
msgstr ""

#: Help_ch_Customize.xml:506(guilabel)
msgid "QIF Import:"
msgstr ""

#: Help_ch_Customize.xml:510(para)
msgid "Show documentation: Show some documentation-only pages in QIF Import druid."
msgstr ""

#: Help_ch_Customize.xml:517(guilabel)
msgid "Generic Importer"
msgstr ""

#: Help_ch_Customize.xml:521(para)
msgid "Checkbox to enable/disable the use Bayesian algorithms to match new transactions with existing accounts."
msgstr ""

#: Help_ch_Customize.xml:526(para)
msgid "Match display threshold: The minimum score a potential match must have to be displayed in the match list."
msgstr ""

#: Help_ch_Customize.xml:531(para)
msgid "Auto-add threshold: A transaction whose best match's score is in the red zone (above the display threshold but below or equal to the Auto-ADD threshold) will be ADDed by default."
msgstr ""

#: Help_ch_Customize.xml:537(para)
msgid "Auto-clear threshold: A transaction whose best match's score is in the green zone (above or equal to the Auto-CLEAR threshold) will be CLEARed by default."
msgstr ""

#: Help_ch_Customize.xml:543(para)
msgid "Commercial ATM fees threshold: In some places commercial ATMs (not belonging to a financial institution) are installed in places like convenience store. These ATM add its fee directly to the amount instead of showing up as a separate transaction or in your monthly banking fees. For example, you withdraw $100, and you are charged $101.50 plus Interac fees. If you manually entered that $100, the amounts won't match. You should set this to whatever is the maximum such fee in your area (in units of your local currency), so the transaction will be recognized as a match."
msgstr ""

#: Help_ch_Customize.xml:559(title)
msgid "Register"
msgstr ""

#: Help_ch_Customize.xml:565(guilabel)
msgid "Actions:"
msgstr ""

#: Help_ch_Customize.xml:569(para)
msgid "'Enter' moves to blank transaction: If checked, pressing the 'Enter' key will move to the blank transaction at the bottom of the register. If clear, pressing the 'Enter' key will move down one row."
msgstr ""

#: Help_ch_Customize.xml:575(para)
msgid "Auto-raise lists: Automatically raise the list of accounts or actions during input."
msgstr ""

#: Help_ch_Customize.xml:583(guilabel)
msgid "Reconciling:"
msgstr ""

#: Help_ch_Customize.xml:587(para)
msgid "Check cleared transactions: Pre-check cleared transactions when creating a reconcile dialog."
msgstr ""

#: Help_ch_Customize.xml:592(para)
msgid "Automatic interest transfer: Prior to reconciling an account which charges or pays interest, prompt the user to enter a transaction for the interest charge or payment. Currently only enabled for Bank, Credit, Mutual, Asset, Receivable, Payable, and Liability accounts."
msgstr ""

#: Help_ch_Customize.xml:599(para)
msgid "Automatic credit card payment: After reconciling a credit card statement, prompt the user to enter a credit card payment."
msgstr ""

#: Help_ch_Customize.xml:606(guilabel)
msgid "Graphics:"
msgstr ""

#: Help_ch_Customize.xml:610(para)
msgid "Use system theme colors: If checked, the system color theme will be applied to register windows. If clear, the original GnuCash register colors will be used."
msgstr ""

#: Help_ch_Customize.xml:616(para)
msgid "Double mode colors alternate with transactions: Alternate the primary and secondary colors by transaction instead of by alternating by row."
msgstr ""

#: Help_ch_Customize.xml:621(para)
msgid "Draw horizontal lines between rows: Show horizontal borders on the cells."
msgstr ""

#: Help_ch_Customize.xml:626(para)
msgid "Draw vertical lines between columns: Show vertical borders on the cells."
msgstr ""

#: Help_ch_Customize.xml:638(title)
msgid "Register Defaults"
msgstr ""

#: Help_ch_Customize.xml:644(guilabel)
msgid "Default Style:"
msgstr ""

#: Help_ch_Customize.xml:648(para)
msgid "Basic ledger: Show all transactions on one line. (Two in double line mode.)"
msgstr ""

#: Help_ch_Customize.xml:653(para)
msgid "Auto-split ledger: Automatically expand the current transaction to show all splits. All other transactions are shown on one line. (Two in double line mode.)"
msgstr ""

#: Help_ch_Customize.xml:659(para)
msgid "Transaction journal: All transactions are expanded to show all splits."
msgstr ""

#: Help_ch_Customize.xml:666(guilabel)
msgid "Other Defaults"
msgstr ""

#: Help_ch_Customize.xml:670(para)
msgid "Register opens in a new window: If checked, each register will be opened in its own top level window. If clear, the register will be opened in the current window."
msgstr ""

#: Help_ch_Customize.xml:676(para)
msgid "Double line mode: Show two lines of information for each transaction instead of one. Does not affect expanded transactions"
msgstr ""

#: Help_ch_Customize.xml:681(para)
msgid "Number of transactions: Show this many transactions in a register. A value of zero means show all transactions."
msgstr ""

#: Help_ch_Customize.xml:686(para)
msgid "Number of Rows: Display this many rows when a register is created."
msgstr ""

