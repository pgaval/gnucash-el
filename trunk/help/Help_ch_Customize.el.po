msgid "This is an screen image of the \"Accounting Period Preferences\" windows."
msgstr ""

#: Help_ch_Customize.xml:133(para)
msgid "This section defines the following preferences;"
msgstr ""

#: Help_ch_Customize.xml:137(para)
msgid "<guilabel>Summary Bar Content</guilabel> - check-boxes to include/exclude; Grand Totals, and Non-Currency totals."
msgstr ""

#: Help_ch_Customize.xml:142(para)
msgid "Grand Totals: If checked, show a grand total of all accounts converted to the default report currency."
msgstr ""

#: Help_ch_Customize.xml:147(para)
msgid "Non-Currency: If checked, non-currency commodities will be shown in the summary bar. If clear, only currencies will be shown."
msgstr ""

#: Help_ch_Customize.xml:154(para)
msgid "<guilabel>Start Date</guilabel> - specify the beginning of the accounting period."
msgstr ""

#: Help_ch_Customize.xml:158(para)
msgid "Relative: Use the specified relative starting date for profit/loss calculations. Also use this date for net assets calculations. Relative defines start by; today or start of current/previous year, quarter, month."
msgstr ""

#: Help_ch_Customize.xml:164(para)
msgid "Absolute - Use the specified absolute starting date for profit/loss calculations. Also use this date for net assets calculations."
msgstr ""

#: Help_ch_Customize.xml:172(para)
msgid "<guilabel>End Date</guilabel> - specify the ending of the accounting period."
msgstr ""

#: Help_ch_Customize.xml:176(para)
msgid "Relative: Use the specified relative ending date for profit/loss calculations. Also use this date for net assets calculations."
msgstr ""

#: Help_ch_Customize.xml:181(para)
msgid "Absolute - Use the specified absolute ending date for profit/loss calculations. Also use this date for net assets calculations."
msgstr ""

