#: Help_ch_Customize.xml:299(guilabel)
msgid "Invoices"
msgstr ""

#: Help_ch_Customize.xml:303(para)
msgid "Open in new window: Check-box to open Invoice in a new window. If checked, each invoice will be opened in its own top level window. If clear, the invoice will be opened in the current window."
msgstr ""

#: Help_ch_Customize.xml:309(para)
msgid "Accumulate splits on post: Whether multiple entries in an invoice which transfer to the same account should be accumulated into a single split by default. This setting can be changed in the Post dialog."
msgstr ""

#: Help_ch_Customize.xml:315(para)
msgid "Number of Rows: This option is used to set the default number of register rows to display in invoices."
msgstr ""

#: Help_ch_Customize.xml:320(para)
msgid "Tax Included?: Whether tax is included by default in entries on Invoices. This setting is inherited by new customers and vendors."
msgstr ""

#: Help_ch_Customize.xml:328(guilabel)
msgid "Bills"
msgstr ""

#: Help_ch_Customize.xml:332(para)
msgid "Notify when due?: Whether to display the list of Bills Due at start-up."
msgstr ""

#: Help_ch_Customize.xml:339(para)
msgid "Days in advance: How many days in the future to warn about Bills coming due."
msgstr ""

#: Help_ch_Customize.xml:346(para)
msgid "Tax Included?: Whether tax is included by default in entries on Bills. This setting is inherited by new customers and vendors."
msgstr ""

#: Help_ch_Customize.xml:357(title)
msgid "Date/Time"
msgstr ""

